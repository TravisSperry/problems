<h1>Listing project_fields</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_fields.each do |project_field| %>
      <tr>
        <td><%= link_to 'Show', project_field %></td>
        <td><%= link_to 'Edit', edit_project_field_path(project_field) %></td>
        <td><%= link_to 'Destroy', project_field, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project field', new_project_field_path %>
