<h1>Listing slider_objects</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @slider_objects.each do |slider_object| %>
      <tr>
        <td><%= link_to 'Show', slider_object %></td>
        <td><%= link_to 'Edit', edit_slider_object_path(slider_object) %></td>
        <td><%= link_to 'Destroy', slider_object, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Slider object', new_slider_object_path %>
