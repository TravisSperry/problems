<div class="content">
  <!--Panel Content-->
  <div class="page-content">
    <div class="back-navigation">
      <%= link_to 'back', root_url, class: 'btn btn-warning' %>
    </div>
    <h1 class="page-title">Results...</h1>
    <div class="horizontal-divider page-title-divider"></div>
    <div class="search-result-panel">
      <% if Rails.env.production? %>
      <p class="no-result">Searching is currently disabled.</p>
      <% else %>
      <% @projects.each do |project| %>
      <%= link_to project.name, project %>
      <hr>
      <% end %>
      <% end %>
    </div>
  </div>
  <!--/.Panel Content-->
</div>
